<?xml version="1.0" ?>
<robot name="widowxl">
  
  <material name="interbotix_black">
    <color rgba="0.2 0.2 0.2 1.0"/>
  </material>

  <link name="widowxl/base_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://my_widowxl_description/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="interbotix_black"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.05"/>
      <geometry>
        <cylinder length="0.1" radius="0.07"/>
      </geometry>
    </collision>
  </link>

  <joint name="waist" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="-3.1415" upper="3.1415" velocity="1.571"/>
    <origin rpy="0 0 0" xyz="0 0 0.167"/>
    <parent link="widowxl/base_link"/>
    <child link="widowxl/shoulder_link"/>
    <dynamics friction="0.1"/>
  </joint>

  <link name="widowxl/shoulder_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.167"/>
      <geometry>
        <mesh filename="package://my_widowxl_description/meshes/shoulder_link__4__1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="interbotix_black"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.05"/> </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 -0.167"/>
      <mass value="0.480879"/>
      <inertia ixx="0.000379" ixy="0.0" ixz="0.0" iyy="0.000555" iyz="0.0" izz="0.000588"/>
    </inertial>
  </link>

  <joint name="shoulder" type="revolute">
    <axis xyz="0 1 0"/>
    <limit effort="20" lower="-1.571" upper="1.571" velocity="1.571"/>
    <origin rpy="0 0 0" xyz="0 0 0.0367"/>
    <parent link="widowxl/shoulder_link"/>
    <child link="widowxl/upper_arm_link"/>
    <dynamics friction="0.1"/>
  </joint>

  <link name="widowxl/upper_arm_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.2"/>
      <geometry>
        <mesh filename="package://my_widowxl_description/meshes/biceps_link_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="interbotix_black"/>
    </visual>
    <collision>
      <origin rpy="0 1.57 0" xyz="0.1 0 0"/>
      <geometry>
        <cylinder length="0.2" radius="0.03"/> </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 -0.2"/>
      <mass value="0.430811"/>
      <inertia ixx="0.003463" ixy="0.0" ixz="0.0" iyy="0.003587" iyz="0.0" izz="0.000456"/>
    </inertial>
  </link>

  <joint name="elbow" type="revolute">
    <axis xyz="0 1 0"/>
    <limit effort="15" lower="-1.571" upper="1.571" velocity="1.571"/>
    <origin rpy="0 0 0" xyz="0.05 0 0.145"/>
    <parent link="widowxl/upper_arm_link"/>
    <child link="widowxl/forearm_link"/>
    <dynamics friction="0.1"/>
  </joint>

  <link name="widowxl/forearm_link">
    <visual>
      <origin rpy="0 0 0" xyz="-0.05 0 -0.347"/>
      <geometry>
        <mesh filename="package://my_widowxl_description/meshes/forearm_link_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="interbotix_black"/>
    </visual>
    <collision>
      <origin rpy="0 1.57 0" xyz="0.15 0 0"/>
      <geometry>
         <cylinder length="0.25" radius="0.025"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="-0.05 0 -0.347"/>
      <mass value="0.297673"/>
      <inertia ixx="0.002176" ixy="0.0" ixz="0.0" iyy="0.000057" iyz="0.0" izz="0.002186"/>
    </inertial>
  </link>

  <joint name="wrist_angle" type="revolute">
    <axis xyz="0 1 0"/>
    <limit effort="5" lower="-1.745" upper="1.745" velocity="1.571"/>
    <origin rpy="0 0 0" xyz="0.15 0 0"/>
    <parent link="widowxl/forearm_link"/>
    <child link="widowxl/wrist_link"/>
    <dynamics friction="0.1"/>
  </joint>

  <link name="widowxl/wrist_link">
    <visual>
      <origin rpy="0 0 0" xyz="-0.2 0 -0.347"/>
      <geometry>
        <mesh filename="package://my_widowxl_description/meshes/wrist_1_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="interbotix_black"/>
    </visual>
    <collision>
       <origin rpy="0 0 0" xyz="0 0 0"/>
       <geometry>
         <box size="0.05 0.03 0.03"/>
       </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="-0.2 0 -0.347"/>
      <mass value="0.084957"/>
      <inertia ixx="0.000030" ixy="0.0" ixz="0.0" iyy="0.000028" iyz="0.0" izz="0.000031"/>
    </inertial>
  </link>

  <joint name="wrist_rotate" type="revolute">
    <axis xyz="1 0 0"/>
    <limit effort="1" lower="-2.167" upper="2.167" velocity="1.571"/>
    <origin rpy="0 0 0" xyz="0.08 0 0"/>
    <parent link="widowxl/wrist_link"/>
    <child link="widowxl/wrist_2_link"/>
    <dynamics friction="0.1"/>
  </joint>

  <link name="widowxl/wrist_2_link">
    <visual>
      <origin rpy="0 0 0" xyz="-0.28 0.0 -0.347"/>
      <geometry>
        <mesh filename="package://my_widowxl_description/meshes/wrist_2_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="interbotix_black"/>
    </visual>
    <inertial>
      <origin rpy="0 0 0" xyz="-0.28 0.0 -0.347"/>
      <mass value="0.072885"/>
      <inertia ixx="0.000025" ixy="0.0" ixz="0.0" iyy="0.000018" iyz="0.0" izz="0.000016"/>
    </inertial>
  </link>

  <joint name="ee_arm" type="fixed">
    <axis xyz="1 0 0"/>
    <origin rpy="0 0 0" xyz="0.043 0 0"/>
    <parent link="widowxl/wrist_2_link"/>
    <child link="widowxl/ee_arm_link"/>
  </joint>

  <link name="widowxl/ee_arm_link">
    <inertial>
      <mass value="0.001"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <joint name="gripper" type="continuous">
    <axis xyz="1 0 0"/>
    <limit effort="1" velocity="3.1415"/>
    <origin rpy="0 0 0" xyz="0.0055 0 0"/>
    <parent link="widowxl/ee_arm_link"/>
    <child link="widowxl/gripper_prop_link"/>
  </joint>

  <link name="widowxl/gripper_prop_link">
    <inertial>
      <mass value="0.00434"/>
      <inertia ixx="0.0000005" ixy="0.0" ixz="0.0" iyy="0.0000011" iyz="0.0" izz="0.0000005"/>
    </inertial>
  </link>
  
  </robot>
